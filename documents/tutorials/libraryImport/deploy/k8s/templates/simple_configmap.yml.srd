kind: ConfigMap
apiVersion: v1
metadata:
  name: "${SIMPLE_LIBRARY_NAME}"
  namespace: "${SIMPLE_LIBRARY_NAMESPACE}"
  labels:
    app.kubernetes.io/name: "${SIMPLE_LIBRARY_NAME}"
    app.kubernetes.io/managed-by: "${SIMPLE_LIBRARY_MANAGER}"
    app.kubernetes.io/version: "$(getConfig "simple.library.version")"
    app.kubernetes.io/component: "${SIMPLE_LIBRARY_NAME}"
    app.kubernetes.io/part-of: "${SIMPLE_LIBRARY_NAME}"
